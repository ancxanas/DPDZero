// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URI")
}

model User {
  id        String @id @default(uuid()) @db.Uuid
  username  String @unique
  email     String @unique
  password  String
  full_name String
  age       Int
  gender    String
}
